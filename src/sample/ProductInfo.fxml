<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox BorderPane.alignment="TOP_LEFT" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="page-title" text="Product Info">
      <VBox.margin>
         <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </VBox.margin></Text>
    <FlowPane>
        <Button mnemonicParsing="false" onAction="#handleBackBtnAction" text="Back">
         <FlowPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </FlowPane.margin></Button>
        <Button mnemonicParsing="false" onAction="#handleEditBtnAction" text="Edit">
         <FlowPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </FlowPane.margin></Button>
    </FlowPane>
    <GridPane>
        <Label text="Picture" GridPane.rowIndex="9">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Label>
        <Label text="Product ID" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Label>
        <Label text="Name" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Label>
        <Label text="Category" GridPane.rowIndex="3">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Label>
        <Label text="Price" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Label>
        <Label text="Created By" GridPane.rowIndex="10">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Label>
        <Label text="Created On" GridPane.rowIndex="11">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Label>
        <ImageView fx:id="productPicView" fitHeight="200.0" fitWidth="200.0" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="9" />
        <Text fx:id="productIdTxt" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Text>
        <Text fx:id="productNameTxt" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Text>
        <Text fx:id="categoryTxt" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Text>
        <Text fx:id="priceTxt" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Text>
        <Text fx:id="createdByUserNameTxt" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="10">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Text>
        <Text fx:id="createdOnTxt" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1" GridPane.rowIndex="11">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin></Text>
      <columnConstraints>
         <ColumnConstraints />
         <ColumnConstraints />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
      </rowConstraints>
    </GridPane>
</VBox>
